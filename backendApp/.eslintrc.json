{   
    "extends": ["eslint:recommended"],
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "rules": {
        "indent": [2, "tab"],
        "func-names": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "no-console": "off",
        "no-debugger":"off"
    }
}